

[<%= @name %>]
	host = <%= @host %>
	port = <%= @port %>
	tds version = <%= @version %>
<%- if @initial_block_size -%>
	initial block size = <%= @initial_block_size %>
<% end -%>
<%- if @try_server_login -%>
	try server login = <%= @try_server_login %>
<% end -%>
<%- if @try_domain_login -%>
	try domain login = <%= @try_domain_login %>
<% end -%>
<%- if @nt_domain -%>
	nt domain = <%= @nt_domain %>
<% end -%>
<%- if @cross_domain_login -%>
	cross domain login = <%= @cross_domain_login %>
<% end -%>
<%- if @dump_file -%>
	dump file = <%= @dump_file %>
<% end -%>
<%- if @dump_file_append -%>
	dump file append = <%= @dump_file_append %>
<% end -%>
<%- if @debug_level -%>
	debug level = <%= @debug_level %>
<% end -%>
<%- if @timeout -%>
	timeout = <%= @timeout %>
<% end -%>
<%- if @connect_timeout %>
	connect timeout = <%= @connect_timeout %>
<% end -%>
<%- if @emulate_little_endian -%>
	emulate little endian = <%= @emulate_little_endian %>
<% end -%>
<%- if @client_charset -%>
	client charset = <%= @client_charset %>
<% end -%>
<%- if @text_size -%>
	text size = <%= @text_size %>
<% end -%>